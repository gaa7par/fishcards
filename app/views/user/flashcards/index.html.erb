<h2>Flashcards</h2>

<ul>
  <% @flashcards.each do |flashcard| %>
  <li>
    <strong><%= flashcard.front %></strong>
    <%= flashcard.back %>
    <%#= link_to 'Show', user_language_flashcard_path(flashcard) %>
  </li>
  <% end %>
</ul>

<%#= link_to 'New flashcard', new_user_language_flashcard_path(@flashcard) %>
<%= link_to 'Back', user_languages_path %>
